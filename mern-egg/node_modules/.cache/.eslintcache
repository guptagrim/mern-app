t</h2>  \n                <form onSubmit={this.onSubmit}>     \n                     <label>Event name: </label>\n                     <input  type=\"text\" required value={this.state.eventName} onChange={this.onChangeEventname} />\n                   \n\n                    <label>Description: </label>\n                    <input  type=\"text\" required value={this.state.description} onChange={this.onChangeDescription} />\n                    <div>\n                        <label>Date: </label>\n                        <DateTimeRangePicker value={this.state.date} onChange={this.onChangeDate} calendarClassName=\"react-datetime-picker__wrapper\"/>\n                        \n                    </div>\n                     <input type=\"submit\" value=\"Create Event\" />\n                </form>\n            </div>    \n        )\n        }\n}","/Users/agrimgupta/Desktop/new/mern-egg/src/components/myevents.component.js",["82"],"import React, { Component } from 'react';\nimport { Link } from 'react-router-dom';\nimport axios from 'axios'\n\n\n\nconst Event = props => (\n    <tr>\n        <td>{props.event.eventname}</td>\n        <td>{props.event.description}</td>\n        <td>{props.event.starttime} until {props.event.endtime}</td>\n        <td>\n            <a href=\"#\" onClick={() => { props.deleteEvent(props.event._id) }}>delete event</a>\n        </td>\n     </tr>\n);\n\nexport default class MyEvents extends Component {\n    constructor(props) {\n        super(props)\n        this.deleteEvent = this.deleteEvent.bind(this)\n        this.state = {\n            events: []\n        }    \n    }\n   \n    componentDidMount(){\n        axios.get(`http://localhost:5000/events`)\n            .then(res => {\n                this.setState({events: res.data});\n            })\n            .catch((error) => {\n                console.log(error);\n              })\n\n    }\n\n    deleteEvent(id){\n        axios.delete('http://localhost:5000/events/'+id)\n            .then(response => { console.log(response.data)});\n  \n        this.setState({\n            events: this.state.events.filter(el => el._id !== id)\n      })    \n    }\n\n    eventList(){\n        return this.state.events.map(currentevent => {\n             return <Event event={currentevent} deleteEvent={this.deleteEvent} key={currentevent._id}/>})\n    }\n\n    render() {\n        return (\n            <div>\n               <h2>Logged Events </h2> \n               <Link to='/home/event'> Add Event </Link>\n \n            <table className=\"table\">\n            <thead className=\"thead-light\">\n                <tr>\n                    <th>Event Name</th>\n                    <th>Description</th>\n                    <th>Duration</th>\n                </tr>\n            </thead>\n                <tbody>\n                { this.eventList() }\n                </tbody>\n            </table>\n            </div>    \n        );\n    }\n}\n","/Users/agrimgupta/Desktop/new/mern-egg/src/components/creategroup.component.js",["83","84"],"import React, { Component } from 'react';\nimport { Link } from 'react-router-dom';\nimport axios from 'axios'\nimport Select from 'react-select'\nimport auth from './auth'\n\nexport default class CreateGroup extends Component {\n    constructor(props) {\n        super(props)\n\n        this.onSubmit = this.onSubmit.bind(this);\n        this.onChangeGroupname = this.onChangeGroupname.bind(this);\n        this.onChangeUsers = this.onChangeUsers.bind(this);\n\n        this.state = {\n            groupName: '',\n            groupMembers: [],\n            groupCount: 0,\n            users: []\n        }    \n    }\n   \n    componentDidMount(){\n        axios.get(`http://localhost:5000/users`)\n            .then(res => {\n\n                this.setState({\n                     users: res.data.map(user=>user.username)\n                })            \n            })\n            .catch((error) => {\n                console.log(error);\n              })\n\n